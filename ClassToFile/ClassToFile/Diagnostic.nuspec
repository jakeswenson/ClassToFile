<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>ClassToFile</id>
    <version>0.7.0</version>
    <title>ClassToFile</title>
    <authors>Jake Swenson</authors>
    <owners>Jake Swenson</owners>
    <licenseUrl>https://raw.githubusercontent.com/jakeswenson/ClassToFile/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jakeswenson/ClassToFile</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A C# code fix to move a class to a new file (or rename to current file to match)</description>
    <copyright>Copyright Jake Swenson</copyright>
    <tags>C#-CodeFix, CodeFix, analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="*.dll" target="tools\analyzers\" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>